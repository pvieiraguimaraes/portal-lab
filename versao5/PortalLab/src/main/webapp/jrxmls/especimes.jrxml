	<parameter name="MUNICIPIO" class="java.lang.String"/>
	<parameter name="LOCALIDADE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	epc.altitude,
	epc.codigo_catalogo,
	epc.codigo_coleta,
	epc.data_coleta,
	epc.latitude,
	epc.longitude,
	epc.localidade,
	epc.numero_individuo,
	epc.observacao,
	epc.observacao_geografia,
	epc.precisao,
	epc.profundidade,
	epc.imprecisao,
	epc.observacao_taxonomia,
	tBaseDatum.descricao AS datum,
	iGeoEst.nome_itgeo AS estado ,
	tBaseEstg.descricao AS estagio_desenvolvimento,
	gEnd.nome AS grupo_endereco,
	lab.nome AS endereco_fisico,
	tBaseMas.descricao AS massa_agua,
	tBaseFen.descricao AS fenologia,
	tBaseCol.descricao AS metodo_coleta,
	iGeoMun.nome_itgeo AS municipio,
	iGeoPais.nome_itgeo AS pais,
	tBaseSex.descricao AS sexo,
	tBaseMont.descricao AS tipo_montagem,
	iTaxRei.nome_ittax AS reino,
	iTaxFil.nome_ittax AS filo,
	iTaxCls.nome_ittax AS classe,
	iTaxSCl.nome_ittax AS sub_classe,
	iTaxOrd.nome_ittax AS ordem,
	iTaxSOr.nome_ittax AS sub_ordem,
	iTaxFam.nome_ittax AS familia,
	iTaxSFa.nome_ittax AS sub_familia,
	iTaxGen.nome_ittax AS genero,
	iTaxEpi.nome_ittax AS epiteto_especifico
FROM especime AS epc
LEFT JOIN tabela_basica AS tBaseDatum
	ON	tabela = 'Datum'
		AND tBaseDatum.id_item = epc.id_datum
LEFT JOIN item_geografico AS iGeoEst
	ON	iGeoEst.id_itgeo = epc.id_estado
LEFT JOIN tabela_basica AS tBaseEstg
	ON	tBaseEstg.tabela = 'estagiodesenvolvimento'
		AND tBaseEstg.id_item = epc.id_estagio_desenvolvimento
LEFT JOIN tabela_basica AS tBaseFen
	ON	tBaseFen.tabela = 'Fenologia'
		AND tBaseFen.id_item = epc.id_fenologia
LEFT JOIN grupo_endereco AS gEnd
	ON	gEnd.id_grupo = epc.id_grupo_endereco_fisico
INNER JOIN laboratorio AS lab
	ON	lab.id_laboratorio = epc.id_laboratorio
LEFT JOIN tabela_basica AS tBaseMas
	ON	tBaseMas.tabela = 'MassaDAgua'
		AND tBaseMas.id_item = epc.id_massaDAgua
LEFT JOIN tabela_basica AS tBaseCol
	ON	tBaseCol.tabela = 'MetodoColeta'
		AND tBaseCol.id_item = epc.id_metodo_coleta
LEFT JOIN item_geografico AS iGeoMun
	ON	iGeoMun.id_itgeo = epc.id_municipio
LEFT JOIN item_geografico AS iGeoPais
	ON	iGeoPais.id_itgeo = epc.id_pais
LEFT JOIN tabela_basica AS tBaseSex
	ON	tBaseSex.tabela = 'Sexo'
		AND tBaseSex.id_item = epc.id_sexo
LEFT JOIN tabela_basica AS tBaseMont
	ON	tBaseMont.tabela = 'TipoDeMontagem'
		AND tBaseMont.id_item = epc.id_tipo_de_montagem
LEFT JOIN item_taxonomico AS iTaxRei
	ON	iTaxRei.id_ittax = epc.id_reino
LEFT JOIN item_taxonomico AS iTaxFil
	ON	iTaxFil.id_ittax = epc.id_filo
LEFT JOIN item_taxonomico AS iTaxCls
	ON	iTaxCls.id_ittax = epc.id_classe
LEFT JOIN item_taxonomico AS iTaxSCl
	ON	iTaxSCl.id_ittax = epc.id_sub_classe
LEFT JOIN	item_taxonomico AS iTaxFam
	ON	iTaxFam.id_ittax = epc.id_familia
LEFT JOIN	item_taxonomico AS iTaxEpi
	ON	iTaxEpi.id_ittax = epc.id_epiteto_especifico
LEFT JOIN	item_taxonomico AS iTaxGen
	ON	iTaxGen.id_ittax = epc.id_genero
LEFT JOIN	item_taxonomico AS iTaxOrd
	ON	iTaxOrd.id_ittax = epc.id_ordem
LEFT JOIN	item_taxonomico AS iTaxSFa
	ON	iTaxSFa.id_ittax = epc.id_sub_familia
LEFT JOIN	item_taxonomico AS iTaxSOr
	ON	iTaxSOr.id_ittax = epc.id_sub_ordem
WHERE 	( 	(
			$P{HAS_WHERE} = 'false'
			OR ( 	($P{TABLE} = 'genero'
				AND epc.id_genero = $P{ID_TABLE})
				OR ($P{TABLE} = 'familia'
				AND epc.id_familia = $P{ID_TABLE})
			 )
		)
		AND  (
			$P{HAS_SECOND_WHERE} = 'false'
			OR (
				($P{TABLE2} = 'grupo'
				AND epc.id_grupo_endereco_fisico = $P{ID_TABLE2}
				)
				OR (
				$P{TABLE2} = 'endereco'
				AND epc.id_laboratorio = $P{ID_TABLE2}
				)
				OR (
				$P{TABLE2} = 'localidade'
				AND (	ISNULL($P{ESTADO}, iGeoEst.id_itgeo) = iGeoEst.id_itgeo
					OR ISNULL($P{MUNICIPIO}, iGeoMun.id_itgeo) = iGeoMun.id_itgeo
					OR ISNULL($P{LOCALIDADE}, epc.localidade) = epc.localidade)
				)
			)
		)
)]]>
	</queryString>
